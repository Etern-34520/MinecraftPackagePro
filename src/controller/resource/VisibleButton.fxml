<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<fx:root type="Pane" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <children>
      <ToggleButton fx:id="visibleButton" maxHeight="60.0" maxWidth="60.0" mnemonicParsing="false" prefHeight="60.0" prefWidth="60.0" />
      <Hyperlink fx:id="backLink" layoutX="30.0" layoutY="40.0" onMousePressed="#windowBack" text="复位" textFill="WHITE" visible="false">
         <font>
            <Font size="9.0" />
         </font>
      </Hyperlink>
   </children>
</fx:root>
